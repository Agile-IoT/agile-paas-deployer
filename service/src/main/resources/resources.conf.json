{
    "heroku":{
        "provider":{
            "name":"Heroku",
            "url":"https://api.heroku.com/",
            "versions":["v3"],
            "defaultVersion":"v3"
        },
        "resourceClass":"eu.atos.paas.resources.HerokuResource",
        "clients":["eu.atos.paas.heroku.HerokuClient"]
    },
    "cloudfoundry":{
        "provider":{
            "name":"CloudFoundry",
            "url":"https://www.example.com",
            "versions":["v2"],
            "defaultVersion":"v2"
        },
        "resourceClass":"eu.atos.paas.resources.CFBasedResource",
        "clients":["eu.atos.paas.cloudfoundry.CloudFoundryClient"]
    },
    "pivotal":{
        "provider":{
            "name":"Pivotal",
            "url":"https://api.run.pivotal.io",
            "versions":["v2"],
            "defaultVersion":"v2"
        },
        "resourceClass":"eu.atos.paas.resources.CFBasedResource",
        "clients":["eu.atos.paas.cloudfoundry.CloudFoundryClient"]
    },
    "bluemix":{
        "provider":{
            "name":"Bluemix",
            "url":"https://api.ng.bluemix.net",
            "versions":["v2"],
            "defaultVersion":"v2"
        },
        "resourceClass":"eu.atos.paas.resources.CFBasedResource",
        "clients":["eu.atos.paas.cloudfoundry.CloudFoundryClient"]
    },
    "openshift":{
        "provider":{
            "name":"OpenShift",
            "url":"http://www.example.com",
            "versions":["v2","v3"],
            "defaultVersion":"v3"
        },
        "resourceClass":"eu.atos.paas.resources.OpenShiftResource",
        "clients":["eu.atos.paas.openshift2.Openshift2Client","eu.atos.paas.openshift3.OpenShift3Client"]
    },
    "openshift.com":{
        "provider":{
            "name":"OpenShift Online",
            "url":"https://openshift.redhat.com",
            "versions":["v2","v3"],
            "defaultVersion":"v2"
        },
        "resourceClass":"eu.atos.paas.resources.OpenShiftResource",
        "clients":["eu.atos.paas.openshift2.Openshift2Client","eu.atos.paas.openshift3.OpenShift3Client"]
    },
    "openwhisk.com":{
        "provider":{
            "name":"Openwhisk Online",
            "url":"https://openwhisk.apache.org/",
            "versions":["v1"],
            "defaultVersion":"v1"
        },
        "resourceClass":"eu.atos.paas.resources.OpenwhiskResource",
        "clients":["eu.atos.deployer.faas.openwhisk.OpenwhiskClient"]
    }
    
}
